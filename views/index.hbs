<h1>lfp</h1>
<input class="form-control" id="input" placeholder=" photo, text, music, video or all">
<ul id="all-posts">
  {{#each posts}}
  <form method="post" action="/delete/{{_id}}"><button>X</button></form>
  <li>
    <div class='edit' contenteditable='true' oid='{{_id}}'>
      {{description}}
    </div>
  </li>
  {{/each}}
</ul>
