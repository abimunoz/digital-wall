<h1>digital<br />wall</h1>

<input class="form-control" id="input" placeholder="multimedia">
<ul id="all-posts">
  {{#each posts}}
  <form method="post" action="/delete/{{_id}}"><button>X</button></form>
  <li>
    <div class="edit" contenteditable="true" id="{{_id}}">
        {{description}}
    </div>
  </li>
  {{/each}}
</ul>
