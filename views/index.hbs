<h1>lfp</h1>
<div class="form">
  <input id="name" placeholder=" name">
  <input id="text" placeholder=" text">
  <input id="media" placeholder=" media">
  <button id="sumbit">sumbit</button>
</div>
<div>
  <button>+</button>
</div>
<ul id="all-posts">
  {{#each posts}}
  <form method="post" action="/delete/{{_id}}"><button>X</button></form>
  <li>
    <div class="edit" contenteditable="true" id="{{_id}}">
      {{text}}
      {{createdAt}}
      <!-- {{media}} -->
      <!-- {{nameAge}} -->
    </div>
  </li>
  {{/each}}
</ul>
